d,GAA6B,UAAUC,EAAV,EAAcC,IAAd,EAAgC;AAAA,MAAZC,EAAY,uEAAP,KAAO;AAC3D,MAAI,QAAQF,EAAR,KAAe,QAAf,IAA2B,QAAQC,IAAR,KAAiB,QAAhD,EAA0D;;AAC1D,OAAK,IAAIE,GAAT,IAAgBH,EAAhB,EAAoB;AAClBA,IAAAA,EAAE,CAACG,GAAD,CAAF,GAAUF,IAAI,CAACG,cAAL,CAAoBD,GAApB,IAA4B,QAAQF,IAAI,CAACE,GAAD,CAAZ,KAAsB,QAAtB,GAAiCE,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,SAAL,CAAeN,IAAI,CAACE,GAAD,CAAnB,CAAX,CAAjC,GAAyEF,IAAI,CAACE,GAAD,CAAzG,GAAkHH,EAAE,CAACG,GAAD,CAA9H;AACD;;AACD,MAAID,EAAJ,EAAQ,OAAOF,EAAP;AACT,CAND;;AAQArB,MAAM,CAAC6B,UAAP,CAAkB,UAACR,EAAD,EAAKC,IAAL,EAAWQ,IAAX,EAAoB;AACpC;AACA;AACA;AACA,MAAIC,OAAO,GAAG,CAAC9B,KAAK,CAAC+B,OAAN,CAAcC,QAAf,GAA0B,KAA1B,GAAkC,IAAhD,CAJoC,CAIkB;AACtD;;AACA,MAAIF,OAAO,IAAI,KAAX,IAAoBV,EAAE,CAACa,IAAH,IAAW,OAAnC,EAA4C;AAC1ClC,IAAAA,MAAM,CAACmC,OAAP,CAAe;AAAEC,MAAAA,IAAI,EAAE;AAAR,KAAf;AACD,GARmC,CASpC;;;AACA,MAAIf,EAAE,CAACa,IAAH,KAAY,OAAhB,EAAyB;AACvB,QAAIH,OAAO,IAAI,KAAf,EAAsB;AACpB/B,MAAAA,MAAM,CAACmC,OAAP,CAAe;AAAEC,QAAAA,IAAI,EAAE;AAAR,OAAf;AACD;AACF;;AAED,MAAIf,EAAE,CAACgB,IAAH,IAAWhB,EAAE,CAACgB,IAAH,CAAQC,QAAvB,EAAiC;AAC/B,QAAIjB,EAAE,CAACgB,IAAH,CAAQC,QAAR,CAAiBC,MAAjB,CAAwB,UAAAhC,GAAG,EAAI;AACjC,aAAON,KAAK,CAAC+B,OAAN,CAAcQ,WAAd,CAA0BC,OAA1B,CAAkClC,GAAlC,MAA2C,CAAC,CAAnD;AACD,KAFG,EAEDmC,MAFC,GAEQ,CAFZ,EAEe;AACbZ,MAAAA,IAAI;AACL,KAJD,MAIO;AACLhC,MAAAA,GAAG,CAACO,SAAJ,CAAcW,QAAd,CAAuBC,OAAvB,CAA+B;AAC7BC,QAAAA,OAAO,EAAE,YADoB;AAE7ByB,QAAAA,OAF6B,qBAEnB;AACRC,UAAAA,OAAO,CAACC,IAAR;AACD;AAJ4B,OAA/B;AAMD;AACF,GAbD,MAaO;AACLf,IAAAA,IAAI;AACL;AAEF,CAjCD;AAmCAhC,GAAG,CAACgD,GAAJ,CAAQ5C,SAAR;AACA,IAAIJ,GAAJ,CAAQ;AACNE,EAAAA,MAAM,EAANA,MADM;AAENC,EAAAA,KAAK,EAALA,KAFM;AAGN8C,EAAAA,MAAM,EAAE,gBAAAC,CAAC;AAAA,WAAIA,CAAC,CAACjD,GAAD,CAAL;AAAA;AAHH,CAAR,EAIGkD,MAJH,CAIU,MAJV","sourcesContent":["import Vue from 'vue'\r\nimport App from './App.vue'\r\nimport router from './router'\r\nimport store from \"./store\";\r\nimport ElementUI from 'element-ui';\r\nimport 'element-ui/lib/theme-chalk/index.css';\r\nimport './style/common.less'; //全局css\r\n\r\nVue.config.productionTip = false\r\n\r\n//复制内容\r\nVue.prototype.$copyTxt = function (val) {\r\n  document.addEventListener('copy', save); // 监听浏览器copy事件 \r\n  document.execCommand('copy'); // 执行copy事件，这时监听函数会执行save函数。 \r\n  document.removeEventListener('copy', save); // 移除copy事件 // 保存方法 \r\n  function save(e) {\r\n    e.clipboardData.setData('text/plain', val); // 剪贴板内容设置 \r\n    Vue.prototype.$message.success({\r\n      message: '内容已复制到剪切板'\r\n    });\r\n    e.preventDefault();\r\n  }\r\n}\r\n\r\n\r\n//取对象的交集\r\nVue.prototype.$ObjectMixed = function (to, from, cb = false) {\r\n  if (typeof (to) != 'object' || typeof (from) != 'object') return;\r\n  for (let key in to) {\r\n    to[key] = from.hasOwnProperty(key) ? (typeof (from[key]) == 'object' ? JSON.parse(JSON.stringify(from[key])) : from[key]) : to[key];\r\n  }\r\n  if (cb) return to;\r\n}\r\n\r\nrouter.beforeEach((to, from, next) => {\r\n  // to: Route: 即将要进入的目标 路由对象\r\n  // from: Route: 当前导航正要离开的路由\r\n  // next: Function: 一定要调用该方法来 resolve 这个钩子。执行效果依赖 next 方法的调用参数。\r\n  let isLogin = !store.getters.getToken ? false : true; // 是否登录\r\n  // 未登录状态；当路由到route指定页时，跳转至login\r\n  if (isLogin == false && to.name != 'login') {\r\n    router.replace({ path: '/login', });\r\n  }\r\n  // 已登录状态；当路由到login时，跳转至main \r\n  if (to.name === 'login') {\r\n    if (isLogin != false) {\r\n      router.replace({ path: '/main', });\r\n    }\r\n  }\r\n\r\n  if (to.meta && to.meta.authList) {\r\n    if (to.meta.authList.filter(val => {\r\n      return store.getters.getAuthList.indexOf(val) !== -1;\r\n    }).length > 0) {\r\n      next();\r\n    } else {\r\n      Vue.prototype.$message.success({\r\n        message: '你没有权限访问该页面',\r\n        onClose() {\r\n          history.back();\r\n        }\r\n      });\r\n    }\r\n  } else {\r\n    next();\r\n  }\r\n\r\n});\r\n\r\nVue.use(ElementUI);\r\nnew Vue({\r\n  router,\r\n  store,\r\n  render: h => h(App)\r\n}).$mount('#app')\r\n"]}]}