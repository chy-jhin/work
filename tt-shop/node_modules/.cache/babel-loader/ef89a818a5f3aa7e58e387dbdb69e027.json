AA,KAAA,IAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA;AACA,UAAA,OAAA,EAAA,GAAA,CAAA,GAAA,IAAA;AADA,SAAA;;AAGA,QAAA,KAAA,CAAA,KAAA,CAAA,eAAA,EAAA;AAAA,UAAA,OAAA,EAAA,IAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AACA,OALA;AAMA,KARA;AASA,IAAA,KATA,mBASA;AACA,WAAA,KAAA,CAAA,eAAA,EAAA;AAAA,QAAA,OAAA,EAAA,KAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KAXA;AAYA,IAAA,MAZA,oBAYA;AACA,MAAA,MAAA,CAAA,MAAA,CAAA,KAAA,KAAA,EAAA,KAAA,QAAA,CAAA,IAAA,EAAA;AACA;AAdA;AAhCA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-dialog\r\n      :title=\"form.id != null?'修改用户':'添加用户'\"\r\n      :visible=\"show === $options.name\"\r\n      :close-on-click-modal=\"false\"\r\n      append-to-body\r\n      @close=\"close\"\r\n      @closed=\"closed\"\r\n      width=\"640px\"\r\n    >\r\n      <el-form class=\"form-box\" size=\"mini\" label-width=\"120px\">\r\n        <el-form-item label=\"登陆账号\">\r\n          <el-input v-model=\"form.login_name\" style=\"width:215px;\" placeholder=\"请输入登录账号\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"关联角色\">\r\n          <el-select\r\n            style=\"width:85%;\"\r\n            size=\"small\"\r\n            v-model=\"form.role_id\"\r\n            multiple\r\n            placeholder=\"请选择关联角色\"\r\n          >\r\n            <el-option v-for=\"item in roleList\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item style=\"width:50%;display:inline-block\" label=\"用户状态\">\r\n          <el-switch v-model=\"form.status\" :active-value=\"1\" :inactive-value=\"2\"></el-switch>\r\n        </el-form-item>\r\n\r\n        <el-form-item label=\"手机号码\">\r\n          <el-input v-model=\"form.mobile\" style=\"width:160px;\" placeholder=\"请输入手机号码\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"登陆密码\">\r\n          <el-input show-password v-model=\"form.pwd\" style=\"width:160px;\" placeholder=\"请输入登陆密码\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button size=\"medium\" @click=\"close\">取 消</el-button>\r\n        <el-button size=\"medium\" type=\"primary\" @click=\"submit\">提 交</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { adminSave as save } from \"@/request/api.js\";\r\nexport default {\r\n  name: \"adminSave\",\r\n  components: {},\r\n  props: {\r\n    value: {\r\n      type: Object\r\n    },\r\n    show: {\r\n      type: String\r\n    }\r\n  },\r\n  watch: {\r\n    show(val) {\r\n      if (val === this.$options.name) {\r\n        this.$ObjectMixed(this.form, this.value.form);\r\n        this.roleList = this.value.roleList;\r\n      }\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      roleList: [],\r\n      form: {\r\n        id: null,\r\n        login_name: \"\",\r\n        mobile: \"\",\r\n        role_id: [],\r\n        status: 1,\r\n        pwd: \"\"\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    submit() {\r\n      save(this.form).then(res => {\r\n        this.$message.success({\r\n          message: res.msg || \"操作成功\"\r\n        });\r\n        this.$emit(\"visibleChange\", { getList: true, show: \"\" });\r\n      });\r\n    },\r\n    close() {\r\n      this.$emit(\"visibleChange\", { getList: false, show: \"\" });\r\n    },\r\n    closed() {\r\n      Object.assign(this.$data, this.$options.data());\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style lang=\"less\" scoped>\r\n</style>"],"sourceRoot":"src/view/config/components"}]}